{
  "name": "wifisurveyor",
  "version": "1.0.0",
  "description": "Visualize Wi-Fi signal strength over a geographic area",
  "main": "app.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build --outDir ../Server/wwwroot --emptyOutDir --assetsDir .",
