    "glslify-deps": "1.3.2",
    "jsdom": "16.5.3",
    "parcel": "1.12.4",
    "ts-mockito": "2.6.1",
    "ts-node": "9.1.1",
    "typescript": "4.2.4",
    "vite": "2.3.0",
    "vite-plugin-html": "2.0.7",
    "vite-plugin-vue2": "1.5.1",
    "vue": "2.6.12",
    "vue-hot-reload-api": "2.3.4",
    "vue-template-compiler": "2.6.12",
    "xunit.ts": "0.8.0"
  },
  "browserslist": [
    "Firefox ESR"
