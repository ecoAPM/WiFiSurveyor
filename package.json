    "Wi-Fi",
    "HeatMap",
    "Heat",
    "Map",
    "Signal",
    "Strength",
    "dBm"
  ],
  "author": "Steve Desmond, ecoAPM <steve@ecoAPM.com> (https://ecoAPM.com)",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ecoAPM/WiFiSurveyor/issues"
  },
  "homepage": "https://github.com/ecoAPM/WiFiSurveyor#readme",
  "devDependencies": {
    "@microsoft/signalr": "5.0.6",
    "@types/delaunator": "3.0.0",
    "@types/jsdom": "16.2.10",
    "@types/node": "14.14.41",
    "@vue/component-compiler-utils": "3.2.0",
    "@vue/test-utils": "1.2.0",
    "delaunator": "4.0.1",
    "global-jsdom": "8.1.0",
